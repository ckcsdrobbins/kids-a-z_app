{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Kids A-Z",
  "version": "0.2",
  "manifest_version": 2,
  "icons": {
    "128": "raz_128.png",
    "16": "raz_16.png"
  },
  "app": {
    "background": {
      "scripts": [ "main.js" ],
      "persistent": true
    }
  },
  "permissions": [
    "webview",
    "storage",
    "audioCapture",
    "videoCapture"
  ],
  "kiosk_enabled": true
}